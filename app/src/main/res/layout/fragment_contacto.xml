<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fragments.ContactoFragment">

    <TextView
        android:text="CONTACTANOS"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:layout_width="173dp"
        android:layout_height="66dp" android:id="@+id/textView1"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.079"/>

    <ImageView
        android:layout_width="167dp"
        android:layout_height="149dp"
        android:contentDescription="TODO"
        app:srcCompat="@drawable/phone" android:id="@+id/call"
        app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.215"/>

    <ImageView
        android:layout_width="167dp"
        android:layout_height="149dp" app:srcCompat="@drawable/mail" android:id="@+id/email"
        app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.416"/>
    <ImageView
        android:layout_width="167dp"
        android:layout_height="149dp" app:srcCompat="@drawable/ubication" android:id="@+id/ubi"
        app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.617"/>
    <ImageView
        android:layout_width="167dp"
        android:layout_height="149dp" app:srcCompat="@drawable/watsap" android:id="@+id/wasap"
        app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.818"/>
</androidx.constraintlayout.widget.ConstraintLayout>